<!doctype html>
<html>

<head>
  <title>GittingStuffDone</title>
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
  <link rel="stylesheet" href="/styles/main.css">
  <script src="/vendor/jquery.js"></script>
</head>

<body>
  <div class="container">
    <div id="headerDiv" style="display: block;" >
      <img src="/images/logo.png" alt="Logo"  style="width:50px;height:50px" >
      <h1><span class="fa fa-anchor"></span> Welcome <%= user.google.name %>! </h1>
    </div>
    <div id="listDiv" style="display: block;">
      <img src="/images/add.png" alt="Add Item" style="" onclick="addItem()">
      <table id="listTable" >
         <thead><tr><th>Todo Item</th></tr></thead>
         <tbody><tr><td>This is my item</td></tr></tbody>
      </table>
    </div>
    <div id="itemDiv" style="display: none;">
      <table id="listTable" >
        <tr><td>Item Description</td></tr>
        <tr><td>This is my item</td></tr>
      </table>
      <img src="/images/save.png" alt="Save" style="" onclick="saveItem()">
    </div>
    <div id="pageOptionsDiv" style="display: block;"></div>
  </div>
  <script src="/scripts/main.js"></script>
</body>

</html>
